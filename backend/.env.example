# Alpaca API (Paper Trading)
ALPACA_API_KEY=your_alpaca_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets

# Supabase
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_key_here
OPENROUTER_API_BASE_URL=https://openrouter.ai/api/v1
# Primary model for trade/market analysis (TESTED: Best quality/speed - 408 score, 1.4s)
OPENROUTER_PRIMARY_MODEL=openai/gpt-oss-safeguard-20b
# Secondary model for copilot chat (TESTED: Fast and quality - 322 score, 1.7s)
OPENROUTER_SECONDARY_MODEL=google/gemini-2.5-flash-preview-09-2025
# Tertiary model for quick insights (TESTED: Good balance - 221 score, 3.4s)
OPENROUTER_TERTIARY_MODEL=openai/gpt-oss-120b
# Backup free model
OPENROUTER_BACKUP_MODEL=minimax/minimax-m2:free
# Temperature for responses (0.0-1.0)
OPENROUTER_TEMPERATURE=0.7

# Perplexity AI API Configuration
PERPLEXITY_API_KEY=your_perplexity_key_here
PERPLEXITY_API_BASE_URL=https://api.perplexity.ai
PERPLEXITY_DEFAULT_MODEL=sonar-pro

# Strategy Configuration
WATCHLIST=SPY,QQQ,AAPL,MSFT,NVDA,TSLA,AMD,GOOG,AMZN,META
MAX_POSITIONS=5
RISK_PER_TRADE_PCT=0.01
CIRCUIT_BREAKER_PCT=0.05
EMA_SHORT=9
EMA_LONG=21
STOP_LOSS_ATR_MULT=2.0
TAKE_PROFIT_ATR_MULT=4.0

# Server
BACKEND_PORT=8000
FRONTEND_URL=http://localhost:5173
LOG_LEVEL=INFO

# Copilot Configuration
COPILOT_CONTEXT_ENABLED=true
COPILOT_HYBRID_ROUTING=true
COPILOT_TRADE_EXECUTION=false
COPILOT_CONTEXT_TIMEOUT_MS=800
COPILOT_AI_TIMEOUT_MS=15000
COPILOT_CACHE_TTL_SECONDS=60
COPILOT_INCLUDE_ACCOUNT=true
COPILOT_INCLUDE_POSITIONS=true
COPILOT_INCLUDE_HISTORY=true
COPILOT_INCLUDE_MARKET=true
COPILOT_INCLUDE_NEWS=true
COPILOT_INCLUDE_RISK=true
COPILOT_HISTORY_TRADES=20
COPILOT_NEWS_LOOKBACK_HOURS=24
COPILOT_NEWS_MAX_ITEMS=12

# Bracket Orders
BRACKET_ORDERS_ENABLED=true
DEFAULT_TAKE_PROFIT_PCT=2.0
DEFAULT_STOP_LOSS_PCT=1.0

# Streaming
STREAMING_ENABLED=true
STREAM_RECONNECT_DELAY=5
